      "@shared-types": [
        "packages/shared-types"
      ],
      "@shared-types/*": [
        "packages/shared-types/*"
      ],
      "@memento/utils": [
        "packages/utils/dist"
      ],
      "@memento/utils/*": [
        "packages/utils/dist/*",
        "packages/utils/dist/src/*",
        "packages/utils/dist/src/*"
      ]
